<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <zetabox.barcode.zcodebarreader.CodeBarReader
        android:id="@+id/barCodeReader"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:code_bar_title="title"
        app:code_bar_description="press QR COde "
        app:code_bar_description_color="@color/dark_white_text"
        app:code_bar_error_description = "description error  "
        app:code_bar_error_title = " ERROR : "
        app:code_bar_prompt = "prompt "
        app:icon_scanner = "@drawable/qr_code"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>